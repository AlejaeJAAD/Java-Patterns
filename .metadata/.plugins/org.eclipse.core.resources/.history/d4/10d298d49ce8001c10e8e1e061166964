package Basic_Pattern;

public class Binary_Rectangle_Pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int length = 7;
		int breadth = 4;
		
		for (int i = 0; i < breadth; i++) {
			for (int j = 0; j < length; j++) {
				if(i%2 == 0) {
					System.out.print("1 ");
				} else {
					System.out.print("0 ");
				}
			}
			System.out.println("");
		}
	}

}
